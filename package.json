      "plugin:@typescript-eslint/recommended"
    ]
  },
  "dependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.29",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/validator": "^12.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
